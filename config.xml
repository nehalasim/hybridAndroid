<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.CC" version="1.6.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DA_CHCP 1.6</name>
    <description>
        Digital Auscultation app for collecting data from Community Clinic by CHCP.
    </description>
    <author email="nehalasim@gmail.com" href="http://cordova.io">
        Asim Ahmed Nehal
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>

    <preference name = "orientation" value = "landscape"/>
</widget>
